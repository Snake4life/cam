_node_complete() {
  local cur_word options
  cur_word="${COMP_WORDS[COMP_CWORD]}"
  if [[ "${cur_word}" == -* ]] ; then
    COMPREPLY=( $(compgen -W '--cpu-prof-interval --experimental-vm-modules --force-context-aware --inspect-brk-node --http-parser --experimental-loader --input-type --redirect-warnings --no-warnings --trace-deprecation --preserve-symlinks-main --experimental-exports --check --security-revert --expose-internals --help --print --es-module-specifier-resolution --tls-min-v1.1 --v8-pool-size --policy-integrity --heap-prof-dir --tls-max-v1.3 --tls-min-v1.0 --version --max-http-header-size --debug-arraybuffer-allocations --heap-prof-name --cpu-prof --experimental-wasm-modules --zero-fill-buffers --experimental-modules --v8-options --trace-event-categories --icu-data-dir --perf-prof-unwinding-info --require --tls-min-v1.2 --frozen-intrinsics --trace-event-file-pattern --perf-basic-prof-only-functions --test-udp-no-try-send --tls-cipher-list --heap-prof-interval --completion-bash --interpreted-frames-native-stack --openssl-config --trace-warnings --napi-modules --use-openssl-ca --use-bundled-ca --tls-max-v1.2 --inspect --title --enable-source-maps --debug-brk --cpu-prof-name --report-directory --trace-sync-io [has_eval_string] --tls-min-v1.3 --report-filename --report-uncaught-exception --experimental-repl-await --experimental-json-modules --throw-deprecation --trace-tls --no-node-snapshot --inspect-publish-uid [ssl_openssl_cert_store] --inspect-brk --report-on-signal --perf-basic-prof --report-signal --interactive --no-force-async-hooks-checks --stack-trace-limit --report-on-fatalerror --pending-deprecation --eval --no-deprecation --preserve-symlinks --debug --track-heap-objects --cpu-prof-dir --experimental-policy --inspect-port --heapsnapshot-signal --perf-prof --abort-on-uncaught-exception --prof-process [has_policy_integrity_string] --unhandled-rejections --http-server-default-timeout --heap-prof --experimental-worker --max-old-space-size --experimental-report --debug= --inspect-brk= --debug-port -c --print <arg> --debug-brk= -e --prof-process -i --inspect= -p --loader -pe -r --security-reverts --trace-events-enabled -v --inspect-brk-node= -h' -- "${cur_word}") )
    return 0
  else
    COMPREPLY=( $(compgen -f "${cur_word}") )
    return 0
  fi
}
complete -F _node_complete node node_g